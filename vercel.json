{
    "version": 2,
    "builds": [
      { "src": "api/convert.py", "use": "@vercel/python" },
      { "src": "pages/index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/convert.py" },
      { "src": "/(.*)", "dest": "pages/index.html" }
    ]
  }
  